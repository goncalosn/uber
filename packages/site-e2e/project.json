{
  "root": "packages/site-e2e",
  "sourceRoot": "packages/site-e2e/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "packages/site-e2e/cypress.json",
        "devServerTarget": "site:serve"
      },
      "configurations": {
        "production": {
          "devServerTarget": "site:serve:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/site-e2e/**/*.{js,ts}"]
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "preset": "conventional",
        "commitMessageFormat": "chore(${projectName}): release version ${version}"
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["site"]
}
