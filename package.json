{
  "name": "uber",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "nx run-many --all --target=build && docker-compose build && docker-compose up -d --force-recreate",
    "start:db": "docker-compose -f docker-compose.dev.yaml up -d",
    "stop": "docker-compose down",
    "test": "npm test:unit && pnpm test:integration",
    "test:unit": "nx run-many --target=test --all -- --group=unit --clearCache=true",
    "test:integration": "nx run-many --target=test --all -- --group=integration --clearCache=true"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm nx affected --target=lint"
    }
  },
